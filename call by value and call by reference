#include<stdio.h>
#include<conio.h>
int swap(int *x,int *y){
    int temp = *x;
    *x = *y;
    *y = temp;
}
int main() {
    int a=5,b=6;
    printf("Enter a: ");
    scanf("%d",&a);
    printf("Enter b: ");
    scanf("%d",&b);
    printf("Before swapping: %d,%d\n",a,b);
    swap(&a,&b);
    printf("After swapping: %d,%d\n",a,b);
    return 0;
    getchar();
}
