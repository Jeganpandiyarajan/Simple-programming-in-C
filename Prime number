#include<stdio.h>
#include<conio.h>
int main(){
    int n,i;
    printf("Enter n : ");
    scanf("%d",&n);
    for(i=2;i<n;i++){
        if(n%i != 0){
            printf("Its a prim number");
            break;
        }
            
        else{
            printf("its not a prime number");
            break;
        }
    }
    getchar();
    return 0;
}
